    <script type="text/ng-template" id="PenawaranView.htm">

        <div></div>

        <div class="col-md-12" ng-init="Init()">
            <div class="panel">
                <div class="list-group-item active">{{Judul}}</div>
                <div class="panel-heading">
                    <div>Tampilkan</div>
                    <form >
                        <label>
                            <input type="radio" ng-model="radioSelected.name" >
                            Semua       
                        </label>
                        <label>
                            <input type="radio" ng-model="radioSelected.name" value="false">
                            Tidak Dipilih       
                        </label>
                        <label>
                            <input type="radio" ng-model="radioSelected.name" value="true">
                            Terpilih    
                        </label><br />
                    </form>

                </div>
                <div class="panel-body">
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>
                                    Kode Pesanan
                                </th>
                                <th>
                                    Jenis Event
                                </th>
                                <th>
                                    Undangan
                                </th>
                                <th>
                                    Status Pesanan
                                </th>
                                <th>
                                    Penawaran
                                </th>
                              
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in Penawarans | filter:{Dipilih:radioSelected.name}">
                                <td>
                                    {{item.Pesanan.KodePemesanan}}
                                </td>
                                <td>
                                    {{item.Pesanan.JenisEvent.Nama}}
                                </td>
                                <td>
                                    {{item.Pesanan.JumlahUndangan}}
                                </td>
                                <td>
                                    {{item.Pesanan.StatusPesanan}}
                                </td>
                                <td>
                                    {{item.Biaya}}
                                </td>

                                <td>
                                    <button data-toggle="modal" data-target="#myModalDetail" ng-click="SelectedItemAction(item)" class="btn btn-info glyphicon glyphicon-list"></button>
                                    <button data-toggle="modal" ng-show="item.Pesanan.StatusPesanan=='Pelaksanaan'" data-target="#myModalProgres" ng-click="SelectedItemAction(item)"  class="btn btn-warning glyphicon glyphicon-check"></button>
                                </td>

                              
                            </tr>

                        </tbody>
                    </table>

                </div>
            </div>
        </div>


        <div class="modal fade" id="myModalDetail" tabindex="-1" role="dialog" aria-labelledby="myModalDetail-label">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalDetail-label">Detail Penawaran</h4>
                    </div>
                    <div class="modal-body">
                       <div ng-bind-html="Selecteditem.DetailPenawaranHtml"></div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" id="myModalProgres" tabindex="-1" role="dialog" aria-labelledby="myModalProgres-label">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="myModalProgres-label">Progress</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <label>Sudah Diantar</label>
                            <input type="checkbox" ng-model="Selecteditem.Diantar" ng-change="ChangeProgress(Selecteditem)" />
                        </div>
                        <div class="form-group">
                            <label>Diterima Oleh</label>
                            <input type="text" ng-model="Selecteditem.Penerima" ng-blur="ChangeProgress(Selecteditem)" />
                        </div>
                        <div class="form-group">
                            <label>Telah Dikembalikan</label>
                            <input type="checkbox" ng-model="Selecteditem.Kembali" ng-change="ChangeProgress(Selecteditem)" />
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>

    </script>

    <script type="text/ng-template" id="PenawaranBidView.htm">
        <div class="list-group-item active">{{Judul}}</div>
        <div class="container-fluid" ng-init="Init()">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Informasi Event
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label">Kode Event</label>
                                {{Pesanan.Pemesanan.KodePemesanan}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Pelanggan</label>
                                {{Pesanan.customer.Nama}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Jenis Event</label>
                                {{Pesanan.JenisEvent.Nama}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Undangan/Peserta</label>
                                {{Pesanan.Pemesanan.JumlahUndangan}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Tanggal Mulai Event</label>
                                {{Pesanan.Pemesanan.TanggalAcara |date:'dd-MM-yyyy HH:mm:ss'}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Tanggal Selesai Event</label>
                                {{Pesanan.Pemesanan.TanggalSelesai |date:'dd-MM-yyyy HH:mm:ss'}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Catatan</label>
                                {{Pesanan.Pemesanan.Catatan}}
                            </div>
                            <div class="form-group">
                                <label class="control-label">Tanggal Pengajuan</label>
                                {{Pesanan.Pemesanan.Tanggal |date:'dd-MM-yyyy HH:mm:ss'}}
                            </div>
                        </div>
                        <div class="panel-footer">
                            <div class="alert alert-warning alert-dismissible" role="alert" ng-show="IsMessageShow">
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                                <strong>Warning!</strong> {{Message}}.
                            </div>

                        </div>
                    </div>


                    <div class="panel panel-default" ng-show="IsShowBid">
                        <div class="panel-heading">
                            Pengajuan Penawaran
                        </div>
                        <div class="panel-body">
                            <div class="form-group">
                                <label class="control-label">Detail Penawaran</label>
                                <textarea ui-tinymce="tinymceOptions" type="text" class="form-control" placeholder="Detail Penawaran" ng-model="model.DetailPenawaran"></textarea>
                            </div>
                            <div class="form-group">
                                <label class="control-label">Biaya</label>
                                <input type="number" class="form-control" placeholder="Placecholder" ng-model="model.Biaya">
                            </div>
                            <div class="form-group">
                                <button type="button" class="btn btn-success" ng-click="Bid(model)">Ajukan</button>
                            </div>

                        </div>
                        <div class="panel-footer">
                        </div>
                    </div>
                </div>
            </div>


        </div>


    </script>

    <!--Kategories View-->
    <script type="text/ng-template" id="LayananView.htm">
    
        <div class="list-group" ng-init="Init()">
            <div class="list-group-item active">{{Judul}}</div>
            <div class="list-group-item">
                <div class="btn-group">

                    <button class="btn btn-primary" type="button" id="modal-13567" ng-click="IsNew=true;layanan=null" href="#modal-container-13567" role="button" data-toggle="modal">
                        <em class="glyphicon glyphicon-plus-sign"></em> Tambah
                    </button>

                </div>
            </div>
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>
                           Status
                        </th>
                        <th>
                            Nama
                        </th>
                        <th>
                            Kategori
                        </th>
                        <th>
                            Harga
                        </th>
                        <th>
                           
                        </th>


                    </tr>
                </thead>
                <tbody>
                    <tr ng-repeat="item in ListLayanan">
                        <td>
                            <input type="checkbox" ng-model="item.Aktif" ng-change="ChangeLayananActived(item)" />
                        </td>
                        <td>
                            {{item.Nama}}
                        </td>
                        <td>
                            {{item.Kategori}}
                        </td>
                        <td>
                            IDR. <span class="pull-right">{{item.Harga|number}}</span>
                        </td>
                        <td>
                            <div class="btn-group pull-right">

                                <button class="btn btn-warning" type="button" ng-click="ChangeSelectedItem(item)"  href="#modal-container-13567" role="button" data-toggle="modal">
                                    <em class="glyphicon glyphicon-edit"></em>Edit
                                </button>
                                <button  ng-click="ChangeSelectedItemDetail(item)" class="btn btn-info" type="button"  id="modal-21978" href="#modal-container-21978" role="button"  data-toggle="modal">
                                    <em class="glyphicon glyphicon-info-sign"></em> Detail
                                </button>
                              
                            </div>
                        </td>

                    </tr>

                </tbody>
            </table>



        </div>


        <div class="modal fade" id="modal-container-21978" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-lg">
                <div class="modal-content">
                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            ×
                        </button>
                        <h4 class="modal-title" id="myModalLabel">
                           Details
                        </h4>
                    </div>
                    <div class="modal-body">
                        <div class="modal-body">
                           
                            <div class="form-group">
                                <label>Nama Layanan</label>  {{layanan.Nama}}
                            </div>
                            <div class="form-group">
                                <label>Kategori</label>  {{layanan.Kategori}}
                            </div>
                            <div class="form-group">
                                <label>Stock</label>
                               {{layanan.Stok}}
                            </div>
                         
                            <div class="form-group">
                                <label>Harga</label>
                                   IDR. {{layanan.Harga|number}}
                            </div>

                            <div class="form-group">
                                <label>Deskripsi Layanan</label>
                               <div id="dir" ng-bind-html="layanan.DescriptionHtml" >

                               </div>
                            </div>



                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">
                            Close
                        </button>
                    </div>
                </div>

            </div>

        </div>
        <div class="modal fade" id="modal-container-13567" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            ×
                        </button>
                        <h4 class="modal-title" id="myModalLabel">
                           Layanan Baru
                        </h4>
                    </div>
                    <form role="form" name="formlayanan" id="formlayanan" ng-submit="SaveLayanan(layanan)">
                        <div class="modal-body">
                            <label>Nama Layanan</label>
                            <div class="form-group">
                                <input required ng-model="layanan.Nama" placeholder="Nama Layanan" type="text" class="form-control" id="categoryName" />
                            </div>
                            <div class="form-group" ng-show="IsNew">
                                <label>Kategori</label>
                                <select name="categoryName" required ng-model="layanan.IdKategori"
                                        class="form-control" id="categoryName">
                                    <option value="" disabled selected hidden>Kategori</option>
                                    <option ng-repeat="option in Categories" value="{{option.Id}}">{{option.Nama}}</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label>Stock</label>
                                <input required ng-model="layanan.Stok" placeholder="Stok" type="number" class="form-control" id="stok" />
                            </div>
                         
                            <div class="form-group">
                                <label>Harga</label>
                                <input required ng-model="layanan.Harga" placeholder="Harga" type="number" class="form-control" id="harga" />
                            </div>
                       

                            <div class="form-group" ng-show="IsNew">
                                <label>Gambar</label>
                                <input type="file" ng-model="file" class="form-control" id="file" name="file" onchange="ShowImage('file','preview')" />
                                <img id="preview" width="300px" height="100px" />
                            </div>


                            <div class="form-group">
                                <label>Deskripsi Layanan</label>
                                <textarea ui-tinymce="tinymceOptions" placeholder="Keterangan" ng-model="layanan.Keterangan" class="form-control" id="element1" name="element1" ></textarea>
                            </div>



                        </div>
                        <div class="modal-footer">

                            <button type="button" class="btn btn-default" data-dismiss="modal">
                                Keluar
                            </button>
                            <button type="submit" class="btn btn-primary">
                                <i ng-show="IsSpinerShow" class="fa fa-spinner fa-spin fa-2x fa-fw"></i>
                                <span class="sr-only">Loading...</span> Simpan
                            </button>
                        </div>
                    </form>
                </div>

            </div>

        </div>

        <div class="modal fade" id="modal-edit" role="dialog" aria-labelledby="myModalLabel1" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">

                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                            ×
                        </button>
                        <h4 class="modal-title" id="myModalLabel1">
                            Edit Kategori
                        </h4>
                    </div>
                    <div class="modal-body">
                        <form role="form">
                            <div class="form-group">
                                <label for="categoryName">
                                    Nama Kategori
                                </label>
                                <input ng-model="category.Nama" type="text" class="form-control" id="categoryNameEdit" />
                            </div>
                            <div class="form-group">
                                <label for="description1">
                                    Keterangan
                                </label>
                                <textarea ng-model="category.Keterangan" class="form-control" id="description1Edit"></textarea>
                            </div>


                        </form>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">
                            Keluar
                        </button>
                        <button type="button" class="btn btn-primary" ng-click="SaveCategory(category)">
                            Simpan
                        </button>
                    </div>
                </div>

            </div>

        </div>


      



    </script>


    <!--Pengusaha View-->
    <script type="text/ng-template" id="PesananView.htm">
        <div class="list-group" ng-init="Init()">
            <div class="list-group-item active">{{Judul}}</div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel-group" id="panel-515627">
                            <div class="panel panel-default" ng-repeat="item in Pesanan">
                                <div class="panel-heading">
                                    <div class="col-md-12">
                                        <div class="col-md-3">
                                            {{item.KodePemesanan}}
                                        </div>
                                        <div class="col-md-3">
                                            {{item.TanggalAcara|date:'dd-MM-yyyy HH:mm:ss'}}
                                        </div>
                                        <div class="col-md-4">
                                            {{item.StatusPesananText}}
                                        </div>
                                        <div class="col-md-2">
                                            <button class="panel-title btn" data-toggle="collapse" data-parent="#panel-515627" href="#panel-element-{{item.KodePemesanan}}">Detail</button>

                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                                <div id="panel-element-{{item.KodePemesanan}}" class="panel-collapse collapse">
                                    <div class="col-md-12">
                                        <div class="panel">
                                            <div class="panel-heading"><h3>Detail Pelanggan</h3></div>
                                            <div class="panel-body">
                                                <div class="col-md-2">
                                                    <h5>Nama</h5>
                                                    <h5>Alamat</h5>
                                                    <h5>Telepon</h5>
                                                </div>
                                                <div class="col-md-4">
                                                    <h5>{{item.Pelanggan.Nama}}</h5>
                                                    <h5>{{item.Pelanggan.Alamat}}</h5>
                                                    <h5>{{item.Pelanggan.Telepon}}</h5>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-12">
                                        <div class="panel">
                                            <div class="panel-heading"><h3>Detail Event</h3></div>
                                            <div class="panel-body">
                                                <div class="col-md-2">
                                                    <h5>Pelaksanaan</h5>
                                                    <h5>Alamat</h5>
                                                    <h5>Catatan</h5>
                                                </div>
                                                <div class="col-md-4">
                                                    <h5>{{item.TanggalAcara|date:'dd-MM-yyyy HH:mm:ss'}}</h5>
                                                    <h5>{{item.Pelanggan.Alamat}}</h5>
                                                    <h5>{{item.Pelanggan.Catatan}}</h5>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

